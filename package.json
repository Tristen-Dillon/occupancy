  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.5.4",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.1.5",
    "prettier": "^3.4.2",
    "sharp": "0.32.6",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  },
  "resolutions": {
    "sharp": "0.30.7"
  }
